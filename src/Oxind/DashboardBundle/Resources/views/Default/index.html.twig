{% extends "::layout.html.twig" %}
{% block content %}

<div class="row">
                {% block title %}

        <div class="col-lg-8">
            <h3> Title </h3>
        </div>

                {% endblock %}

                {% block signin %}
        <div class="col-sm-4">
            New and returning users may <a href="{{ path('fos_user_security_login')}}" data-toggle="modal">Sign In</a>
        </div>
                {% endblock %}
    </div>  

    <div class="container">
                {% block feedbackcontent %}
                {{ include ('OxindFeedbackBundle:Feedback:feedback_search_form.html.twig', { 'feedbacktype_id': feedbacktype.getId() } ) }}
                {% render (controller('OxindFeedbackBundle:Feedback:feedbackfilter' ,{'feedbacktype_id' : feedbacktype.getId() })) %}
                {{ render(controller('OxindFeedbackBundle:Feedback:listFeedback', {'feedbacktype_id' : feedbacktype.getId() })) }}                    
                {% endblock %}
                {% block feedbackform %}
                {{ render(controller('OxindFeedbackBundle:Feedback:getFeedbackForm' , {'feedbacktype_id' : feedbacktype.getId() })) }}                       
            <div class="feedback-form"> 

                <div class="feedback-form-container">
            {% for  val in   feedbackTypes%}               
                        <ul>
                            <li>
                                <a href="{{ path('oxind_dashboard',{'feedbacktype_name': val['name'] } ) }}">{{  val['name'] }}</a>            
                            </li>
                        </ul>    
            {% endfor %}
                    </div>
                </div>
                {% endblock %}

            </div>


{% endblock %}

{% block javascripts %}
{{ parent() }}
            <script src="{{ asset('bundles/oxindfeedback/js/search.js') }}" type="text/javascript"  ></script>
{% endblock %}